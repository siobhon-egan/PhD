	file = {Sperling et al_2020_Microbiome Composition and Borrelia Detection in Ixodes scapularis Ticks at the.pdf:/Users/siobhonegan/Zotero/storage/7RJXBL5Z/Sperling et al_2020_Microbiome Composition and Borrelia Detection in Ixodes scapularis Ticks at the.pdf:application/pdf},
