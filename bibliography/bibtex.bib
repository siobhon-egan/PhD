	author = {Ortiz, Paola A. and Garcia, Herakles A. and Lima, Luciana and da Silva, Flavia Maia and Campaner, Marta and Pereira, Carlos L. and Jittapalapong, Sathaporn and Neves, Luis and Desquesnes, Marc and Camargo, Erney P. and Teixeira, Marta M.G.},
