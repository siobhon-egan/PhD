	pages = {431--443},
	file = {Hamilton et al_2005_A new lineage of trypanosomes from Australian vertebrates and terrestrial.pdf:/Users/siobhonegan/Zotero/storage/IMU6YMSV/Hamilton et al_2005_A new lineage of trypanosomes from Australian vertebrates and terrestrial.pdf:application/pdf},
