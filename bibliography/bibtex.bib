	file = {Taylor et al_2020_Invasive rabbits host immature Ixodes ticks at the urban-forest interface.pdf:/Users/siobhonegan/Zotero/storage/TI78GWEF/Taylor et al_2020_Invasive rabbits host immature Ixodes ticks at the urban-forest interface.pdf:application/pdf},
