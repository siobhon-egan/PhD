@article{panEhrlichialikeOrganismGene2003,
	title = {Ehrlichia-like {Organism} {Gene} {Found} in {Small} {Mammals} in the {Suburban} {District} of {Guangzhou} of {China}},
	volume = {990},
	abstract = {Ehrlichia was described originally in Algeria in 1935, and named formally in 1945. The organism is a small, obligate intracellular bacterium, which replicates within phagosomes in the cytoplasms of monocytes, granulocytes, or thromb- ocytes. It can infect human beings, dogs, horses, cattle, sheep, deer, cats, and mice, and leads to ehrlichiosis. Animal ehrlichiosis caused by the pathogen has now been reported throughout the world and at higher frequencies in tropical and subtropical regions. In Guangzhou, a city in the south of China, an outbreak of canine ehrli- chiosis was first described at a canine feed lot in 1998.1 The causative agents of this disease were identified soon after that: E. canis and E. platys caused the epi- demic disease simultaneously.2 A survey of epidemiology demonstrated that the positive rate of the serum against a recombinant E. chaffeensis p28, which was col- lected from the dogs around the district, reached 50\%. E. canis DNA was found in the brown dog ticks (Rhipicephalus sanguineus) and Boophilus microplus from this area. It is obvious that canine ehrlichiosis is endemic in south of China. In this report we describe an Ehrlichia-like bacterium gene found in small mammals that were captured in this region.},
	journal = {Annals of the New York Academy of Sciences},
	author = {Pan, H. and Liu, S. and Ma, Y. and Tong, S. and Sun, Y.},
	year = {2003},
	pages = {107--111},
