@article{hemidaLackDetectionMiddle2021,
	title = {Lack of detection of the {Middle} {East} respiratory syndrome coronavirus ({MERS}-{CoV}) nucleic acids in some {Hyalomma} dromedarii infesting some {Camelu} dromedary naturally infected with {MERS}-{CoV}},
