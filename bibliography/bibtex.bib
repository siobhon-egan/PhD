@article{mcinnesNovelTrypanosomeTrypanosoma2011,
	title = {Novel trypanosome \textit{{Trypanosoma} {Gilletti}} sp. (euglenozoa: {Trypanosomatidae}) and the extension of the host range of \textit{trypanosoma {Copemani}} to include the koala ( \textit{phascolarctos {Cinereus}} )},
