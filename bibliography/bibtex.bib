@article{andersonEhrlichiaChaffeensisNew1991,
	title = {Ehrlichia chaffeensis, a new species associated with human ehrlichiosis},
	volume = {29},
	doi = {10.1128/JCM.29.12.2838-2842.1991},
	abstract = {The bacterial 16S rRNA genes from blood samples of two patients with human ehrlichiosis and from an isolate recovered from one of the patients were amplified by using the polymerase chain reaction. The amplimers were then cloned and sequenced. The 16S rRNA gene sequence was also determined for Ehrlichia canis (two strains), E. equi, E. phagocytophila (two strains), and E. sennetsu (two strains). These sequences, alongwithapreviouslypublished16SrRNA genesequenceofE.ristici,werecompared.The 16SrRNA gene sequences were identical for al three sources of the human ehrlichiosis agent. The sequence comparisons indicate that the human ehrlichiosis agent is a new species most closely related to E. canis (98.2\%) and more distantlyrelatedtootherEhrlichiaspp.We proposethatthisspeciesbenamedEhrlichiachaffeensissp.nov., with the Arkansas strain as the type strain.},
