@article{andrewsDistributionDispersionAmblyomma2007,
	title = {The distribution and dispersion of {Amblyomma} triguttatum triguttatum on {Yorke} {Peninsula}, {South} {Australia}},
	volume = {12},
	issn = {1362-1971},
	doi = {10.11158/saa.12.1.1},
	abstract = {The kangaroo tick, Amblyomma triguttatum triguttatum, is a major enzootic vector of Coxiella burnetti, the agent causing Q fever, in Australia. This is a widespread tick species previously reported from Queensland, New South Wales and Western Australia. There is also a small population, probably introduced, at the foot of Yorke Peninsula in South Australia. Our aim was to determine both the broad distribution of this tick on Yorke Peninsula and the pattern of dispersion between habitats. No free living A. t. triguttatum were found outside of Innes and Warrenben national parks, although records from humans are available from up to 180 km away. Within the parks, the tick is patchily spread in at least 8 of the 15 habitats searched but is most common in the disturbed areas in and around camping sites. We suggest that the behaviour of the tick's main host, the western grey kangaroo, Macropus fuliginosus, is responsible for its dispersion pattern.},
	language = {English},
	number = {1},
	journal = {Systematic and Applied Acarology},
	author = {Andrews, Ross H. and Petney, Trevor N. and Sherman, Nicole A. and McDiarmid, Louise A. and Dixon, Bruce R.},
	month = apr,
	year = {2007},
	keywords = {Amblyomma triguttatum triguttatum, dispersion, distribution, Q fever, South Australia},
	pages = {3--11},
	file = {Andrews et al_2007_The distribution and dispersion of Amblyomma triguttatum triguttatum on Yorke.pdf:/Users/siobhonegan/Zotero/storage/QTSX7259/Andrews et al_2007_The distribution and dispersion of Amblyomma triguttatum triguttatum on Yorke.pdf:application/pdf;saa.12.1:/Users/siobhonegan/Zotero/storage/3YLX2JB8/saa.12.1.html:text/html},
}

