@article{hosseiniDoesImpactBiodiversity2017,
	title = {Does the impact of biodiversity differ between emerging and endemic pathogens? {The} need to separate the concepts of hazard and risk},
	volume = {372},
	issn = {1471-2970 (Electronic) 0962-8436 (Linking)},
	doi = {10.1098/rstb.2016.0129},
	abstract = {Biodiversity is of critical value to human societies, but recent evidence that biodiversity may mitigate infectious-disease risk has sparked controversy among researchers. The majority of work on this topic has focused on direct assessments of the relationship between biodiversity and endemic-pathogen prevalence, without disentangling intervening mechanisms; thus study outcomes often differ, fuelling more debate. Here, we suggest two critical changes to the approach researchers take to understanding relationships between infectious disease, both endemic and emerging, and biodiversity that may help clarify sources of controversy. First, the distinct concepts of hazards versus risks need to be separated to determine how biodiversity and its drivers may act differently on each. This distinction is particularly important since it illustrates that disease emergence drivers in humans could be quite different to the general relationship between biodiversity and transmission of endemic pathogens. Second, the interactive relationship among biodiversity, anthropogenic change and zoonotic disease risk, including both direct and indirect effects, needs to be recognized and accounted for. By carefully disentangling these interactions between humans' activities and pathogen circulation in wildlife, we suggest that conservation efforts could mitigate disease risks and hazards in novel ways that complement more typical disease control efforts.This article is part of the themed issue 'Conservation, biodiversity and infectious disease: scientific evidence and policy implications'.},
	number = {1722},
	journal = {Philosophical Transactions of The Royal Society Of London. Series B: Biological Sciences},
	author = {Hosseini, P. R. and Mills, J. N. and Prieur-Richard, A. H. and Ezenwa, V. O. and Bailly, X. and Rizzoli, A. and Suzan, G. and Vittecoq, M. and Garcia-Pena, G. E. and Daszak, P. and Guegan, J. F. and Roche, B.},
	month = jun,
	year = {2017},
	pmid = {28438918},
	keywords = {*Biodiversity, Animals, biodiversity, Communicable Diseases, emerging infectious disease, Emerging/*epidemiology/etiology, hazards, Humans, pathogen, Prevalence, Proportional Hazards Models, Risk, Zoonoses/*epidemiology/etiology, zoonotic},
	file = {Hosseini et al_2017_Does the impact of biodiversity differ between emerging and endemic pathogens.pdf:/Users/siobhonegan/Zotero/storage/5X8IB2XP/Hosseini et al_2017_Does the impact of biodiversity differ between emerging and endemic pathogens.pdf:application/pdf},
}

@article{rothenburgerEnvironmentalFactorsZoonotic2017,
	title = {Environmental {Factors} and {Zoonotic} {Pathogen} {Ecology} in {Urban} {Exploiter} {Species}},
	issn = {1612-9210 (Electronic) 1612-9202 (Linking)},
	doi = {10.1007/s10393-017-1258-5},
	abstract = {Knowledge of pathogen ecology, including the impacts of environmental factors on pathogen and host dynamics, is essential for determining the risk that zoonotic pathogens pose to people. This review synthesizes the scientific literature on environmental factors that influence the ecology and epidemiology of zoonotic microparasites (bacteria, viruses and protozoa) in globally invasive urban exploiter wildlife species (i.e., rock doves [Columba livia domestica], European starlings [Sturnus vulgaris], house sparrows [Passer domesticus], Norway rats [Rattus norvegicus], black rats [R. rattus] and house mice [Mus musculus]). Pathogen ecology, including prevalence and pathogen characteristics, is influenced by geographical location, habitat, season and weather. The prevalence of zoonotic pathogens in mice and rats varies markedly over short geographical distances, but tends to be highest in ports, disadvantaged (e.g., low income) and residential areas. Future research should use epidemiological approaches, including random sampling and robust statistical analyses, to evaluate a range of biotic and abiotic environmental factors at spatial scales suitable for host home range sizes. Moving beyond descriptive studies to uncover the causal factors contributing to uneven pathogen distribution among wildlife hosts in urban environments may lead to targeted surveillance and intervention strategies. Application of this knowledge to urban maintenance and planning may reduce the potential impacts of urban wildlife-associated zoonotic diseases on people.},
	journal = {Ecohealth},
	author = {Rothenburger, J. L. and Himsworth, C. H. and Nemeth, N. M. and Pearl, D. L. and Jardine, C. M.},
	month = jun,
	year = {2017},
	pmid = {28631116},
	keywords = {Columba livia domestica, Disease ecology, Mus, Passer domesticus, Rattus, Zoonoses},
	file = {Rothenburger et al_2017_Environmental Factors and Zoonotic Pathogen Ecology in Urban Exploiter Species.pdf:/Users/siobhonegan/Zotero/storage/PYRYWGQA/Rothenburger et al_2017_Environmental Factors and Zoonotic Pathogen Ecology in Urban Exploiter Species.pdf:application/pdf},
