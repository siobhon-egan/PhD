@article{mukhachevaBacteriaFamilyCandidatus2017,
	title = {Bacteria of the {Family} '{Candidatus} {Midichloriaceae}' in {Sympatric} {Zones} of {Ixodes} {Ticks}: {Genetic} {Evidence} for {Vertical} {Transmission}},
	volume = {74},
	issn = {1432-184X (Electronic) 0095-3628 (Linking)},
	doi = {10.1007/s00248-017-0932-z},
	abstract = {Ixodes ticks transmit infectious agents and also harbor their own parasites and symbionts. The presumptive endosymbiont of Ixodes ricinus, 'Candidatus Midichloria mitochondrii', has a unique ability to invade mitochondria within tick ovarian cells and is transovarially transmitted with 100\% efficiency. A closely related bacterium, provisionally named Montezuma (now 'Candidatus Lariskella arthropodarum'), was isolated from the Ixodes persulcatus ticks and human blood in 2004 as well as from Ixodes pavlovskyi in 2015. These microorganisms belong to the family 'Candidatus Midichloriaceae fam. nov.' and were detected not only in tick salivary glands, but also in animal blood. Nevertheless, the relative importance of vertical and horizontal routes for their transmission or maintenance in natural tick populations remains unclear. We analyzed the prevalence of L. arthropodarum and M. mitochondrii in two sympatric zones, where I. persulcatus/I. ricinus and I. persulcatus/I. pavlovskyi cohabit and produce interspecific hybrids. A specificity of the associations of L. arthropodarum with I. persulcatus (100\%) and M. mitochondrii with I. ricinus (96.2\%) was observed in the sympatric zone in Estonia, possibly showing poor contribution of the horizontal route to the overall prevalence of endosymbionts. L. arthropodarum was observed probably multiplying in I. pavlovskyi and also subjected to transovarial transmission, but much less efficiently compared to I. persulcatus. We revealed two new genetic variants of the rrl-rrf intergenic spacer of L. arthropodarum isolated from I. pavlovskyi ticks that possibly could indicate an ongoing process of adaptation of the microorganism to a new host species.},
	number = {1},
	journal = {Microbial Ecology},
	author = {Mukhacheva, T. A. and Kovalev, S. Y.},
	month = jul,
	year = {2017},
	pmid = {28091705},
	keywords = {*Ixodes, *Lariskella arthropodarum, *Midichloria mitochondrii, *Symbiosis, *Sympatric zone, *Vertical transmission, Alphaproteobacteria/*genetics, Animals, Estonia, Genetic Variation, Ixodes/classification/*microbiology, Sympatry},
	pages = {185--193},
	file = {Mukhacheva_Kovalev_2017_Bacteria of the Family 'Candidatus Midichloriaceae' in Sympatric Zones of.pdf:/Users/siobhonegan/Zotero/storage/QK4WLIDU/Mukhacheva_Kovalev_2017_Bacteria of the Family 'Candidatus Midichloriaceae' in Sympatric Zones of.pdf:application/pdf},
}

@article{doughtyMegafaunaDeclineHave2020,
	title = {Megafauna decline have reduced pathogen dispersal which may have increased emergent infectious diseases},
	url = {http://biorxiv.org/lookup/doi/10.1101/2020.01.21.914531},
	doi = {10.1101/2020.01.21.914531},
	abstract = {Abstract The Late Quaternary extinctions of megafauna (defined as animal species {\textgreater}44.5 kg) reduced the dispersal of seeds and nutrients, and likely also microbes and parasites. Here we use body-mass based scaling and range maps for extinct and extant mammal species to show that these extinctions led to an almost seven-fold reduction in the movement of gut-transported microbes, such as Escherichia coli (3.3 km 2 /day to 0.5 km 2 /day). Similarly, the extinctions led to a seven-fold reduction in the mean home ranges of vector-borne pathogens (7.8 km 2 to 1.1 km 2 ). We created an individual-based model where maximum aggregated microbial mutations increased 4-fold after 20,000 years following an order of magnitude decrease in home range. We hypothesize that pathogen speciation and hence endemism increased with isolation, as global dispersal distances decreased through a mechanism similar to the theory of island biogeography. To investigate if such an effect could be found, we analysed where 145 zoonotic diseases have emerged in human populations and found significant improvement (increasing variance explained by 8\%) when we included quantitative estimates of reduced dispersal of ectoparasites and fecal pathogens. These results suggest that megafauna extinctions reduced pathogen dispersal, which increased the development of zoonotic pathogens moving into human populations.},
	language = {English},
	urldate = {2020-02-06},
	author = {Doughty, Christopher E. and Prys-Jones, Tomos and Faurby, Soren and Abraham, Andrew and Hepp, Crystal and Leshyk, Victor and Fofanov, Viacheslav Y. and Nieto, Nathan C. and Svenning, Jens-Christian and Galetti, Mauro},
	month = jan,
	year = {2020},
	file = {Doughty et al. - 2020 - Megafauna decline have reduced pathogen dispersal:/Users/siobhonegan/Zotero/storage/6YK5QKSW/Doughty et al. - 2020 - Megafauna decline have reduced pathogen dispersal .pdf:application/pdf;Doughty et al. - 2020 - Megafauna decline have reduced pathogen dispersal:/Users/siobhonegan/Zotero/storage/28SCCNQD/Doughty et al. - 2020 - Megafauna decline have reduced pathogen dispersal .pdf:application/pdf},
}

@article{gundiBartonellaRattaustralianiSp2009,
	title = {Bartonella rattaustraliani sp. {Nov}., {Bartonella} queenslandensis sp. {Nov}. and {Bartonella} coopersplainsensis sp. {Nov}., identified in {Australian} rats},
	volume = {59},
	issn = {1466-5026 (Print) 1466-5026 (Linking)},
	doi = {10.1099/ijs.0.002865-0},
	abstract = {A total of 11 Bartonella isolates were recovered from the blood of Melomys, Uromys and Rattus species in Australia and were characterized using phenotypic and genotypic methods. Comparison of 16S rRNA gene, ftsZ, gltA and 16S-23S rRNA internal transcribed spacer region sequences from the isolates indicated that they formed three sequence similarity groups that were distinct from one another and from the currently recognized Bartonella species. Phylogenetic analysis based on alignment of concatenated sequences inferred distinct evolutionary lineages for each of the three groups within the genus Bartonella. On the basis of these data, we propose the isolates be accommodated in three novel Bartonella species, namely Bartonella rattaustraliani sp. nov. (type strain AUST/NH4(T) =CIP 109051(T) =CCUG 52161(T) =CSUR B609(T)), Bartonella queenslandensis sp. nov. (type strain AUST/NH12(T) =CIP 109057(T) =CCUG 52167(T) =CSUR B617(T)) and Bartonella coopersplainsensis sp. nov. (type strain AUST/NH20(T) =CIP 109064(T) =CCUG 52174(T) =CSUR B619(T)).},
	number = {Pt 12},
	journal = {International journal of systematic and evolutionary microbiology},
	author = {Gundi, V. A. and Taylor, C. and Raoult, D. and La Scola, B.},
	month = dec,
	year = {2009},
	pmid = {19628592},
	keywords = {16S/genetics, Animals, Bacterial Proteins/genetics, Bacterial/genetics, Bartonella/*classification/genetics/*isolation \& purification, Blood/microbiology, DNA, Molecular Sequence Data, Phylogeny, Ribosomal, Ribosomal Spacer/genetics, RNA, Rodentia/*microbiology},
	pages = {2956--61},
	file = {Gundi et al_2009_Bartonella rattaustraliani sp.pdf:/Users/siobhonegan/Zotero/storage/IH8H3IJM/Gundi et al_2009_Bartonella rattaustraliani sp.pdf:application/pdf},
