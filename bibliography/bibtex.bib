@article{vilcinsBartonellalikeDNADetected2009,
	title = {Bartonella-like {DNA} detected in {Ixodes} tasmani ticks ({Acari}: {Ixodida}) infesting {Koalas} ({Phascolarctos} cinereus) in {Victoria}, {Australia}},
	volume = {9},
