@article{hussain-yusufScreeningRickettsiaCoxiella2020,
	title = {Screening for {Rickettsia}, {Coxiella} and {Borrelia} {Species} in {Ticks} from {Queensland}, {Australia}},
	volume = {9},
	issn = {2076-0817},
	doi = {10.3390/pathogens9121016},
	abstract = {Tick bites in Australia are linked to the transmission of a variety of infectious diseases in humans, livestock and wildlife. Despite this recognition, little is currently known about the variety of potential pathogens that are carried and transmitted by Australian ticks. In this study, we attempted to expand knowledge of Australian tick-borne bacterial pathogens by analyzing various tick species from the state of Queensland for potential human pathogens belonging to the Rickettsia, Coxiella and Borrelia genera. A total of 203 ticks, comprising of four genera and nine different tick species, were screened by specific qPCR assays. An overall Rickettsia qPCR positivity of 6.4\% (13/203) was detected with rickettsial DNA found in four tick species (Ixodes holocyclus, I. tasmani, Amblyomma triguttatum, and Haemaphysalis longicornis). Amplification and analysis of several rickettsial genes from rickettsial qPCR positive samples identified sequences closely related to but genetically distinct from several previously described cultured and uncultured rickettsial species in the Rickettsia spotted fever subgroup. No ticks were positive for either Coxiella or Borrelia DNA. This work suggests that a further diversity of rickettsiae remain to be described in Australian ticks with the full importance of these bacteria to human and animal health yet to be elucidated.},
	language = {English},
	number = {12},
	journal = {Pathogens},
	author = {Hussain-Yusuf, Hazizul and Stenos, John and Vincent, Gemma and Shima, Amy and Abell, Sandra and Preece, Noel D. and Tadepalli, Mythili and Hii, Sze Fui and Bowie, Naomi and Mitram, Kate and Graves, Stephen},
	month = dec,
	year = {2020},
	pages = {1016},
	file = {Hussain-Yusuf et al_2020_Screening for Rickettsia, Coxiella and Borrelia Species in Ticks from.pdf:/Users/siobhonegan/Zotero/storage/XQDKUIFZ/Hussain-Yusuf et al_2020_Screening for Rickettsia, Coxiella and Borrelia Species in Ticks from.pdf:application/pdf},
}

@article{cookScrubTyphusOther1967,
	title = {Scrub typhus and other infections in {North} {Queensland} animals},
	volume = {61},
	abstract = {Investigations at the Queensland Institute of Medical Research (QIMR) Field Station from 1951 to 1955 defined the clinical features and the epidemiology of scrub typhus in man in North Queensland (DOHERTY,1956 ; DERRICK,1961). CARLEY,et al. (1955) described the characteristics of 31 strains of Rickettsia tsutsugamushi isolated from patients in this area. Further studies were carried out in the period August 1961 to July 1964, to determine the importance of forest rodents and marsupials as reservoirs of infection, and as hosts for the suspected mite vector Leptotrombidium deliense. The present paper reports the isolation of strains of R. tsutsugamushi and other infective agents from these animals; data on the distribution of mites collected on the animals have been submitted for publication (DOMROW, 1967). Results of the examina- tion for Leptospirainfection of some animals from this study have been published (BATTEY, et al., 1964).},
