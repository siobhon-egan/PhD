	year = {2011},
	pages = {e26357},
	file = {Aplin et al. - 2011 - Multiple Geographic Origins of Commensalism and Co:/Users/siobhonegan/Zotero/storage/H8QDJ9LV/Aplin et al. - 2011 - Multiple Geographic Origins of Commensalism and Co.pdf:application/pdf;Figure_S1:/Users/siobhonegan/Zotero/storage/K2DYJFTD/Figure_S1.doc:application/msword;References_S1:/Users/siobhonegan/Zotero/storage/NRQQXEI6/References_S1.doc:application/msword;Table_S1:/Users/siobhonegan/Zotero/storage/YI8VKSFD/Table_S1.doc:application/msword;Table_S2:/Users/siobhonegan/Zotero/storage/TGBIM6PL/Table_S2.doc:application/msword;Table_S3:/Users/siobhonegan/Zotero/storage/8QGCXQDD/Table_S3.doc:application/msword;Table_S4:/Users/siobhonegan/Zotero/storage/IVTRW4MZ/Table_S4.doc:application/msword;Table_S5:/Users/siobhonegan/Zotero/storage/2DB9EPSH/Table_S5.doc:application/msword},
