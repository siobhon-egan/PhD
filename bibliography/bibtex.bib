	keywords = {Anaplasma phagocytophilum/*isolation \& purification, Anaplasmataceae, Anaplasmosis/epidemiology/*microbiology, Animals, Ehrlichia/classification/*isolation \& purification, Ehrlichiosis/epidemiology/*microbiology, Eml, Female, Humans, Ixodes, Ixodidae/*microbiology, Male, Nymph, rickettsiales, Tick Infestations/epidemiology/veterinary, ticks, Wisconsin/epidemiology},
	pages = {229--236},
	file = {Murphy et al_2017_Prevalence and Distribution of Human and Tick Infections with the Ehrlichia.pdf:/Users/siobhonegan/Zotero/storage/V82JGWGW/Murphy et al_2017_Prevalence and Distribution of Human and Tick Infections with the Ehrlichia.pdf:application/pdf},
