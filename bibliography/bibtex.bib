@article{clementOutAfricaOrigins2020,
	title = {Out of {Africa}: {The} origins of the protozoan blood parasites of the \textit{{Trypanosoma} {Cruzi}{\textless}i{\textgreater} clade found in bats from }\textit{africa}{\textless}/i{\textgreater}},
