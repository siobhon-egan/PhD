	file = {Wass et al. - 2019 - Cultivation of the causative agent of human neoehr:/Users/siobhonegan/Zotero/storage/XWW84J3N/Wass et al. - 2019 - Cultivation of the causative agent of human neoehr.pdf:application/pdf;Wass et al. - 2019 - Cultivation of the causative agent of human neoehr:/Users/siobhonegan/Zotero/storage/CMQG3IKC/Wass et al. - 2019 - Cultivation of the causative agent of human neoehr.pdf:application/pdf},
}

@article{anderssonCoinfectionCandidatusNeoehrlichia2013,
	title = {Co-infection with '{Candidatus} {Neoehrlichia} {Mikurensis}' and {Borrelia} afzelii in {Ixodes} ricinus ticks in southern {Sweden}},
	volume = {13},
	issn = {1557-7759 (Electronic) 1530-3667 (Linking)},
	doi = {10.1089/vbz.2012.1118},
	abstract = {The tick-borne bacterium 'Candidatus Neoehrlichia mikurensis' has recently been recognized as a human pathogen in Europe and appears to be the second most common pathogenic bacterium in Ixodes ricinus ticks in central Europe, second to Borrelia afzelii. Here, we investigate the prevalence of 'Candidatus N. mikurensis' in host-seeking ticks in southern Sweden and the rate of co-infection with B. afzelii. We developed a real-time qPCR assay targeting the groEL gene of 'Candidatus N. mikurensis' and applied this assay to 949 I. ricinus ticks collected at several locations over 2 years. We found an overall prevalence of 6.0\%, which means that Candidatus N. mikurensis is one of the most common tick-transmitted zoonotic agents in this area. Co-infections with both 'Candidatus N. mikurensis' and B. afzelii occurred in 2.1\% of the ticks, which is significantly more than expected under random co-occurrence. The infection intensity (number of bacterial cells) of 'Candidatus N. mikurensis' was not affected by co-infection with B. afzelii, and vice versa. We conclude that there is a risk for simultaneous transmission of these 2 tick-borne pathogens. The potential medical consequences of this require further investigation.},
	number = {7},
	journal = {Vector Borne Zoonotic Diseases},
	author = {Andersson, M. and Bartkova, S. and Lindestad, O. and Raberg, L.},
	month = jul,
	year = {2013},
	keywords = {Anaplasmataceae Infections/epidemiology/*microbiology, Anaplasmataceae/genetics/*isolation \& purification, Animals, Arachnid Vectors/*microbiology, Bacterial Proteins/genetics, Bacterial/genetics, Borrelia burgdorferi Group/genetics/*isolation \& purification, Chaperonin 60/genetics, Coinfection, DNA, Humans, Ixodes/*microbiology, Lyme Disease/epidemiology/*microbiology, Prevalence, Real-Time Polymerase Chain Reaction, Sweden/epidemiology, Zoonoses},
	pages = {438--42},
	file = {Anderson et al 2013 - Neoehrlichia Borrelia I1:/Users/siobhonegan/Zotero/storage/4KT884M7/Anderson et al 2013 - Neoehrlichia Borrelia I1.pdf:application/pdf;Anderson et al 2013 - Neoehrlichia Borrelia Ix:/Users/siobhonegan/Zotero/storage/TLQANRRS/Anderson et al 2013 - Neoehrlichia Borrelia Ix.pdf:application/pdf},
