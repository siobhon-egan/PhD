@article{austenFirstReportTrypanosoma2020,
	title = {First report of \textit{{Trypanosoma} {Dionisii}} (trypanosomatidae) {Identified} in australia},
	volume = {147},
	issn = {0031-1820, 1469-8161},
	url = {https://www.cambridge.org/core/product/identifier/S0031182020001845/type/journal_article},
	doi = {10.1017/S0031182020001845},
	abstract = {Trypanosomes are blood-borne parasites that can infect a variety of different vertebrates, including animals and humans. This study aims to broaden scientific knowledge about the presence and biodiversity of trypanosomes in Australian bats. Molecular and morphological analysis was performed on 86 blood samples collected from seven different species of microbats in Western Australia. Phylogenetic analysis on 18S rDNA and glycosomal glyceraldehyde phosphate dehydrogenase (gGAPDH) sequences identified Trypanosoma dionisii in five different Australian native species of microbats; Chalinolobus gouldii, Chalinolobus morio, Nyctophilus geoffroyi, Nyctophilus major and Scotorepens balstoni. In addition, two novel, genetically distinct T. dionisii genotypes were detected and named T. dionisii genotype Aus 1 and T. dionisii genotype Aus 2. Genotype Aus 2 was the most prevalent and infected 20.9\% (18/86) of bats in the present study, while genotype Aus 1 was less prevalent and was identified in 5.8\% (5/86) of Australian bats. Morphological analysis was conducted on trypomastigotes identified in blood films, with morphological parameters consistent with trypanosome species in the subgenus Schizotrypanum. This is the first report of T. dionisii in Australia and in Australian native bats, which further contributes to the global distribution of this cosmopolitan bat trypanosome.},
	language = {English},
	number = {14},
	urldate = {2020-10-21},
	journal = {Parasitology},
	author = {Austen, Jill M. and Van Kampen, Esther and Egan, Siobhon L. and O'Dea, Mark A. and Jackson, Bethany and Ryan, Una M. and Irwin, Peter J. and Prada, Diana},
	month = sep,
	year = {2020},
	pages = {1--35},
	file = {Austen et al. - 2020 - First report of Trypanosoma dionisii (Trypa:/Users/siobhonegan/Zotero/storage/VWC5LC2P/Austen et al. - 2020 - First report of Trypanosoma dionisii (Trypa.pdf:application/pdf;Austen et al. - 2020 - First report of Trypanosoma dionisii (Trypa:/Users/siobhonegan/Zotero/storage/LBXPSDJR/Austen et al. - 2020 - First report of Trypanosoma dionisii (Trypa.pdf:application/pdf;First_report_of_&lt\;i&gt\;Trypa:/Users/siobhonegan/Zotero/storage/9V6YSXMS/First_report_of_&lt\;i&gt\;Trypa.pdf:application/pdf;S0031182020001845sup001:/Users/siobhonegan/Zotero/storage/V3H8W9CK/S0031182020001845sup001.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;S0031182020001845sup002:/Users/siobhonegan/Zotero/storage/XCZPF6IB/S0031182020001845sup002.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document},
