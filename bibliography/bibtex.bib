	file = {Ghafar et al. - 2020 - Targeted Next-Generation Sequencing and Informatic:/Users/siobhonegan/Zotero/storage/W3MUSXED/Ghafar et al. - 2020 - Targeted Next-Generation Sequencing and Informatic.pdf:application/pdf;Ghafar et al. - 2020 - Targeted Next-Generation Sequencing and Informatic:/Users/siobhonegan/Zotero/storage/7CL7HP69/Ghafar et al. - 2020 - Targeted Next-Generation Sequencing and Informatic.pdf:application/pdf;microorganisms-1024596 supplementary1:/Users/siobhonegan/Zotero/storage/NHAXKNUA/microorganisms-1024596 supplementary1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Supplementary File 2:/Users/siobhonegan/Zotero/storage/MNHPW5DM/Supplementary File 2.xlsx:application/vnd.openxmlformats-officedocument.spreadsheetml.sheet},
