}

@article{merinoSarcocystidMisidentifiedHepatozoon2008,
	title = {A {Sarcocystid} {Misidentified} as \textit{{Hepatozoon} {Didelphydis}} : {Molecular} {Data} from a parasitic {Infection} in the blood of the southern {Mouse} {Opossum} ( \textit{{Thylamys} {Elegans}} ) from chile},
