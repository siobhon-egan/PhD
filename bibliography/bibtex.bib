	pages = {139--44},
	file = {Glatz et al_2014_Detection of Candidatus Neoehrlichia mikurensis, Borrelia burgdorferi sensu.pdf:/Users/siobhonegan/Zotero/storage/GURJP4M7/Glatz et al_2014_Detection of Candidatus Neoehrlichia mikurensis, Borrelia burgdorferi sensu.pdf:application/pdf},
